{
    "rust-analyzer.files.exclude": [
        ".flatpak"
    ],
    "rust-analyzer.server.extraEnv": {
        "PATH": "/var/home/${env:USER}/.cargo/bin:/app/bin:/usr/bin"
    },
    "rust-analyzer.cargo.buildScripts.overrideCommand": [
        "/var/home/${env:USER}/.cargo/bin/cargo",
        "check",
        "--workspace"
    ]
}